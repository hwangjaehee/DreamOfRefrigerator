{{>layouts/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ArticleId</th>
        <th scope="col">Title</th>
        <th scope="col">Date</th>
        <th scope="col">Contents</th>
    </tr>
    </thead>
    <tbody>
    {{#articleList}}
        <tr>
            <th>{{articleId}}</th>
            <!-- 제목에 링크 걸기 -->
            <td><a href="/articles/{{articleId}}">{{title}}</a></td>
            <td>{{date}}</td>
            <td>{{contents}}</td>
        </tr>
    {{/articleList}}
    </tbody>
</table>

<a href=/articles/new>New Article</a>
{{>layouts/footer}}



